    "NewDialogTemplate": {
        "prefix": "NewDialogTemplate",
        "body": [
            "VSDlgSetDialogTitle('${1:title}')",
            "",
            "local dlgStatus",
            "local field = {}",
            "",
            "field.name = '${2:}'",
            "field.caption = '${3:}'",
            "field.initialVal = '${4:}'",
            "field.type = ${5:0}",
            "dlgStatus = VSDlgAddField(field)",
            "",
            "-- 继续在此处添加对话框选项(模板代码)",
            "",
            "dlgStatus = VSDlgDoModal()",
            "if (dlgStatus == 2) then",
            "    return 0",
            "end",
            "if ((dlgStatus ~= 1) and (dlgStatus ~= 2)) then",
            "    return 1",
            "end"
        ],
        "description": "创建对话框模板代码"
    },
